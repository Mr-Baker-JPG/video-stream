generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("NEXT_PUBLIC_DATABASE_URL")
}

model Access {
  key         String
  /// The value of this field is generated by the database as: `'2021-01-27 12:13:42.848168'::timestamp without time zone`.
  accessed_at DateTime @default(dbgenerated())
  ip          String?
  /// The value of this field is generated by the database as: `uuid_generate_v4()`.
  id          String   @id @default(dbgenerated())
  link        Link     @relation(fields: [key], references: [key])
}

model Link {
  /// The value of this field is generated by the database as: `uuid_generate_v4()`.
  key        String   @id @default(dbgenerated())
  email      String
  created_at DateTime @default(now())
  access     Access[]
}
